script/source = "extends AudioStreamPlayer

var _counter = 0
export var eat_sounds: Array
export var death_sound: AudioStream
export var turn: AudioStream
export var jump: AudioStream

func eat():
    self.stream = eat_sounds[_counter]
    self.play()
    
    _counter += 1
    _counter = _counter % len(eat_sounds)


func die():
    self.stream = death_sound
    self.play()

func jump():
    self.stream = jump
    self.play()

func turn():
    self.stream = turn
    self.play()
